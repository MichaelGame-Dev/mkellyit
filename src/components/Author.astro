<div class="card-elevated">
<p>Michael</p>
<div class="flex gap-4 items-center justify-center">
    {socialLinks
    .filter((social) => !social.hidden)
    .map((social) => (
    <a
      href={social.url}
      target={social.name !== 'RSS' && social.name !== 'Email' ? '_blank' : undefined}
      aria-label={social.name}
      rel={social.name !== 'RSS' && social.name !== 'Email' ? 'me noopener noreferrer' : undefined}
      class="inline-block text-gray-700 dark:text-zinc-400 hover:text-blue-600 dark:hover:text-blue-400 transition-colors"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        viewBox={social.viewBox}
        class="w-6 h-6"
        aria-hidden="true"
        set:html={social.icon}
      />
    </a>
  ))}
</div>
</div>
